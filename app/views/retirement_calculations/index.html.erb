<div class="page-header">
  <h1>
    Retirement calculations
    <a href="/retirement_calculations/new" class="btn btn-lg btn-success">New Retirement calculation</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Assumptions</th>
        <th>Calculation by month</th>
        <th>Actions</th>
      </tr>

      <% @retirement_calculations.each do |retirement_calculation| %>
      <tr>
        <td><%= retirement_calculation.assumptions.age_id %></td>
        <td><%= retirement_calculation.calculation_by_month %></td>
        <td>
          <a href="/retirement_calculations/<%= retirement_calculation.id %>" class="btn btn-primary">Show</a>
          <a href="/retirement_calculations/<%= retirement_calculation.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_retirement_calculation/<%= retirement_calculation.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
