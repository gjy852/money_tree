<div class="page-header">
  <h1>
    Users
    <a href="/users/new" class="btn btn-lg btn-success">New User</a>
  </h1>
</div><div class="row mb-1">
  <div class="col-md-12">
    <%= search_form_for @q, :class => "collapse", :id => "users_filters" do |f| %>
      <p class="lead">Narrow results:</p>

        <div class="form-group">
  <%= f.label :age, "Age" %>
  <div class="row">
    <div class='col-md-6 mb-1'>
      <%= f.text_field :age_gteq, :class => "form-control", :placeholder => "Age greater than or equal to" %>
    </div>
    <div class='col-md-6'>
      <%= f.text_field :age_lteq, :class => "form-control", :placeholder => "Age less than or equal to" %>
    </div>
  </div>
</div>

<div class="form-group">
    <%= f.label :email_cont, "Email contains" %>
    <%= f.text_field :email_cont, :class => "form-control", :placeholder => "Email contains" %>
  </div>

<div class="form-group">
    <%= f.label :current_income_cont, "Current income contains" %>
    <%= f.text_field :current_income_cont, :class => "form-control", :placeholder => "Current income contains" %>
  </div>

<div class="form-group">
  <%= f.label :annual_savings, "Annual savings" %>
  <div class="row">
    <div class='col-md-6 mb-1'>
      <%= f.text_field :annual_savings_gteq, :class => "form-control", :placeholder => "Annual savings greater than or equal to" %>
    </div>
    <div class='col-md-6'>
      <%= f.text_field :annual_savings_lteq, :class => "form-control", :placeholder => "Annual savings less than or equal to" %>
    </div>
  </div>
</div>

<div class="form-group">
    <%= f.label :individual_retirement_goal_cont, "Individual retirement goal contains" %>
    <%= f.text_field :individual_retirement_goal_cont, :class => "form-control", :placeholder => "Individual retirement goal contains" %>
  </div>

<div class="form-group">
    <%= f.label :last_name_cont, "Last name contains" %>
    <%= f.text_field :last_name_cont, :class => "form-control", :placeholder => "Last name contains" %>
  </div>

<div class="form-group">
    <%= f.label :first_name_cont, "First name contains" %>
    <%= f.text_field :first_name_cont, :class => "form-control", :placeholder => "First name contains" %>
  </div>

<div class="form-group">
          <%= f.label :created_at, "Created at" %>
          <div class="row">
            <div class='col-md-6 mb-1'>
              <div class='input-group date' id='created_atgteqPicker'>
                  <%= f.text_field :created_at_gteq, :class => "form-control", :placeholder => "Created at greater than or equal to" %>
                  <span class="input-group-addon">
                      <span class="glyphicon glyphicon-calendar"></span>
                  </span>
              </div>
            </div>

            <div class='col-md-6'>
              <div class='input-group date' id='created_atlteqPicker'>
                  <%= f.text_field :created_at_lteq, :class => "form-control", :placeholder => "Created at less than or equal to" %>
                  <span class="input-group-addon">
                      <span class="glyphicon glyphicon-calendar"></span>
                  </span>
              </div>
            </div>
          </div>
      </div>
      <script type="text/javascript">
          $(function () {
              $('#created_atgteqPicker').datetimepicker({
                  format: 'YYYY-MM-DD hh:mm:SS'
              });
              $('#created_atlteqPicker').datetimepicker({
                  useCurrent: false,
                  format: 'YYYY-MM-DD hh:mm:SS'
              });
              $('#created_atgteqPicker').on("dp.change", function (e) {
                  $('#created_atlteqPicker').data("DateTimePicker").minDate(e.date);
              });
              $('#created_atlteqPicker').on("dp.change", function (e) {
                  $('#created_atgteqPicker').data("DateTimePicker").maxDate(e.date);
              });
          });
      </script>

        <div class="form-group">
  <%= f.label :age_id, "RetirementAssumption Age" %>
  <div class="row">
    <div class='col-md-6 mb-1'>
      <%= f.text_field :retirement_assumptions_age_id_gteq, :class => "form-control", :placeholder => "RetirementAssumption Age greater than or equal to" %>
    </div>
    <div class='col-md-6'>
      <%= f.text_field :retirement_assumptions_age_id_lteq, :class => "form-control", :placeholder => "RetirementAssumption Age less than or equal to" %>
    </div>
  </div>
</div>

<div class="form-group">
          <%= f.label :created_at, "Asset Created at" %>
          <div class="row">
            <div class='col-md-6 mb-1'>
              <div class='input-group date' id='assetsgteqcreated_atPicker'>
                  <%= f.text_field :assets_created_at_gteq, :class => "form-control", :placeholder => "Asset Created at greater than or equal to" %>
                  <span class="input-group-addon">
                      <span class="glyphicon glyphicon-calendar"></span>
                  </span>
              </div>
            </div>

            <div class='col-md-6'>
              <div class='input-group date' id='assetslteqcreated_atPicker'>
                  <%= f.text_field :assets_created_at_lteq, :class => "form-control", :placeholder => "Asset Created at less than or equal to" %>
                  <span class="input-group-addon">
                      <span class="glyphicon glyphicon-calendar"></span>
                  </span>
              </div>
            </div>
          </div>
      </div>
      <script type="text/javascript">
          $(function () {
              $('#assetsgteqcreated_atPicker').datetimepicker({
                  format: 'YYYY-MM-DD hh:mm:SS'
              });
              $('#assetslteqcreated_atPicker').datetimepicker({
                  useCurrent: false,
                  format: 'YYYY-MM-DD hh:mm:SS'
              });
              $('#assetsgteqcreated_atPicker').on("dp.change", function (e) {
                  $('#assetslteqcreated_atPicker').data("DateTimePicker").minDate(e.date);
              });
              $('#assetslteqcreated_atPicker').on("dp.change", function (e) {
                  $('#assetsgteqcreated_atPicker').data("DateTimePicker").maxDate(e.date);
              });
          });
      </script>

<div class="form-group">
          <%= f.label :created_at, "Liability Created at" %>
          <div class="row">
            <div class='col-md-6 mb-1'>
              <div class='input-group date' id='liabilitiesgteqcreated_atPicker'>
                  <%= f.text_field :liabilities_created_at_gteq, :class => "form-control", :placeholder => "Liability Created at greater than or equal to" %>
                  <span class="input-group-addon">
                      <span class="glyphicon glyphicon-calendar"></span>
                  </span>
              </div>
            </div>

            <div class='col-md-6'>
              <div class='input-group date' id='liabilitieslteqcreated_atPicker'>
                  <%= f.text_field :liabilities_created_at_lteq, :class => "form-control", :placeholder => "Liability Created at less than or equal to" %>
                  <span class="input-group-addon">
                      <span class="glyphicon glyphicon-calendar"></span>
                  </span>
              </div>
            </div>
          </div>
      </div>
      <script type="text/javascript">
          $(function () {
              $('#liabilitiesgteqcreated_atPicker').datetimepicker({
                  format: 'YYYY-MM-DD hh:mm:SS'
              });
              $('#liabilitieslteqcreated_atPicker').datetimepicker({
                  useCurrent: false,
                  format: 'YYYY-MM-DD hh:mm:SS'
              });
              $('#liabilitiesgteqcreated_atPicker').on("dp.change", function (e) {
                  $('#liabilitieslteqcreated_atPicker').data("DateTimePicker").minDate(e.date);
              });
              $('#liabilitieslteqcreated_atPicker').on("dp.change", function (e) {
                  $('#liabilitiesgteqcreated_atPicker').data("DateTimePicker").maxDate(e.date);
              });
          });
      </script>

<div class="form-group">
  <%= f.label :user_id, "HistoricalNetWorth User" %>
  <div class="row">
    <div class='col-md-6 mb-1'>
      <%= f.text_field :historical_net_worths_user_id_gteq, :class => "form-control", :placeholder => "HistoricalNetWorth User greater than or equal to" %>
    </div>
    <div class='col-md-6'>
      <%= f.text_field :historical_net_worths_user_id_lteq, :class => "form-control", :placeholder => "HistoricalNetWorth User less than or equal to" %>
    </div>
  </div>
</div>

      <%= f.submit :class => "btn btn-primary btn-block mb-1" %>

      <a href="/users" class="btn btn-default btn-block mb-1">Clear filters</a>
    <% end %>
    <a class="btn btn-default btn-block mb-1" data-toggle="collapse" href="#users_filters">
      Show Filters
    </a>
  </div>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Age</th>
        <th>First name</th>
        <th>Last name</th>
        <th>Individual retirement goal</th>
        <th>Annual savings</th>
        <th>Current income</th>
        <th>Email</th>
        <th>Password</th>
        <th>Actions</th>
      </tr>

      <% @users.each do |user| %>
      <tr>
        <td><%= user.age %></td>
        <td><%= user.first_name %></td>
        <td><%= user.last_name %></td>
        <td><%= user.individual_retirement_goal %></td>
        <td><%= user.annual_savings %></td>
        <td><%= user.current_income %></td>
        <td><%= user.email %></td>
        <td><%= user.password %></td>
        <td>
          <a href="/users/<%= user.id %>" class="btn btn-primary">Show</a>
          <a href="/users/<%= user.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_user/<%= user.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
    <%= paginate @users, theme: 'twitter-bootstrap-4' %>
  </div>
</div>
